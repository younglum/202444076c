#include <stdio.h>

void main(){
    int x,y;
    char op;
    scanf("%d %c %d", &x, &op, &y);
    switch (op)
    {
    case '+':
        printf("%d %c %d = %d", x,op,y,x+y);
        break;
    case '-':
        printf("%d %c %d = %d", x,op,y,x-y);
        break;
    case '*':
        printf("%d %c %d = %d", x, op ,y, x*y);
        break;
    case '/':
        printf("%d %c %d = %d", x, op, y, x/y);
        break;
    default:
        break;
    }
    
}

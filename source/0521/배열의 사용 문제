#include <stdio.h>

int main(){
    int cnt, max=0;
    scanf("%d", &cnt);
    int a[cnt];
    int value=0;
    for (int i = 0; i < cnt; i++)
    {
        scanf("%d", &a[i]);
         if (max < a[i])
        {
            max=a[i];
            value++;
        }
    }
    for (int j = cnt-1; j >=0; j--)
    {
        printf("%d", a[j]);
    }
    printf("보이는 값 계수 : %d", value);
    
}
